<?xml version="1.0" encoding="UTF-8"?>
<includes>
  <default type="image">
    <texture/>
    <aspectratio>stretch</aspectratio>
  </default>
  <default type="multiimage">
    <aspectratio align="center">scale</aspectratio>
    <fadetime>800</fadetime>
    <timeperimage>7000</timeperimage>
    <randomize>true</randomize>
  </default>
  <default type="label">
    <label/>
    <align>left</align>
    <font>font_small_defaults</font>
    <textcolor>dialog_fg_70</textcolor>
    <focusedcolor>dialog_fg_70</focusedcolor>
    <selectedcolor>selected</selectedcolor>
    <disabledcolor>dialog_fg_30</disabledcolor>
    <scroll>false</scroll>
    <scrollspeed>40</scrollspeed>
  </default>
  <default type="fadelabel">
    <align>left</align>
    <font>font_small_defaults</font>
    <textcolor>dialog_fg_70</textcolor>
    <focusedcolor>selected</focusedcolor>
    <selectedcolor>$VAR[ColorHighlight]</selectedcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
  </default>
  <default type="button">
    <aligny>center</aligny>
    <font>font_small_defaults</font>
    <textoffsetx>30</textoffsetx>
    <textcolor>dialog_fg_70</textcolor>
    <focusedcolor>selected</focusedcolor>
    <disabledcolor>dialog_fg_30</disabledcolor>
    <selectedcolor>selected</selectedcolor>
    <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
    <texturenofocus colordiffuse="dialog_bg_30">common/white.png</texturenofocus>
    <pulseonselect>false</pulseonselect>
  </default>
  <default type="togglebutton">
    <textcolor>dialog_fg_70</textcolor>
    <focusedcolor>selected</focusedcolor>
    <selectedcolor>$VAR[ColorHighlight]</selectedcolor>
    <disabledcolor>dialog_fg_30</disabledcolor>
    <texturefocus colordiffuse="dialog_bg_12">common/white.png</texturefocus>
    <texturenofocus/>
    <alttexturefocus colordiffuse="dialog_bg_12">common/white.png</alttexturefocus>
    <alttexturenofocus/>
    <pulseonselect>false</pulseonselect>
    <font>font_small_defaults</font>
  </default>
  <default type="textbox">
    <align>left</align>
    <textcolor>white</textcolor>
    <font>font_small_defaults</font>
  </default>
  <default type="progress">
    <texturebg colordiffuse="2f5f5f5f" border="4">diffuse/progress-bg.png</texturebg>
    <lefttexture colordiffuse="$VAR[ColorHighlight]" border="4">diffuse/progress-left.png</lefttexture>
    <midtexture colordiffuse="$VAR[ColorHighlight]" border="4">diffuse/progress-end.png</midtexture>
    <righttexture colordiffuse="$VAR[ColorHighlight]" border="4">diffuse/progress-right.png</righttexture>
    <overlaytexture/>
  </default>
  <default type="spincontrol">
    <textureup colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-up.png</textureup>
    <texturedown colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-down.png</texturedown>
    <textureupfocus>buttons/spinctrl/arrow-light-up.png</textureupfocus>
    <texturedownfocus>buttons/spinctrl/arrow-light-down.png</texturedownfocus>
    <textureupdisabled colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-up.png</textureupdisabled>
    <texturedowndisabled colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-down.png</texturedowndisabled>
    <align>right</align>
    <width>77</width>
    <height>33</height>
    <font>font_small_defaults</font>
    <reverse>yes</reverse>
    <textcolor>dialog_fg_70</textcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
    <subtype>text</subtype>
    <textoffsetx>-5</textoffsetx>
    <textoffsety>0</textoffsety>
    <pulseonselect>no</pulseonselect>
  </default>
  <default type="spincontrolex">
    <spinwidth>32</spinwidth>
    <spinheight>60</spinheight>
    <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">buttons/button-fo.png</texturefocus>
    <texturenofocus border="40">buttons/button-nofo.png</texturenofocus>
    <textureup colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-up.png</textureup>
    <texturedown colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-down.png</texturedown>
    <textureupfocus>buttons/spinctrl/arrow-light-up.png</textureupfocus>
    <texturedownfocus>buttons/spinctrl/arrow-light-down.png</texturedownfocus>
    <textureupdisabled colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-up.png</textureupdisabled>
    <texturedowndisabled colordiffuse="dialog_fg_30">buttons/spinctrl/arrow-light-down.png</texturedowndisabled>
    <font>font_small_defaults</font>
    <textcolor>dialog_fg_70</textcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
    <textoffsetx>7</textoffsetx>
    <aligny>center</aligny>
    <reverse>yes</reverse>
    <pulseonselect>no</pulseonselect>
  </default>
  <default type="radiobutton">
    <radiowidth>120</radiowidth>
    <radioheight>76</radioheight>
    <textureradioonfocus colordiffuse="selected">buttons/radio-button-on.png</textureradioonfocus>
    <textureradioonnofocus colordiffuse="dialog_fg_70">buttons/radio-button-on.png</textureradioonnofocus>
    <textureradioofffocus colordiffuse="selected">buttons/radio-button-off.png</textureradioofffocus>
    <textureradiooffnofocus colordiffuse="dialog_fg_70">buttons/radio-button-off.png</textureradiooffnofocus>
    <textureradioondisabled colordiffuse="dialog_fg_12">buttons/radio-button-on.png</textureradioondisabled>
    <textureradiooffdisabled colordiffuse="dialog_fg_12">buttons/radio-button-off.png</textureradiooffdisabled>
    <texturefocus border="21" colordiffuse="$VAR[ColorHighlight]">buttons/button-fo.png</texturefocus>
    <texturenofocus border="21">buttons/button-nofo.png</texturenofocus>
    <font>font_small_defaults</font>
    <textcolor>dialog_fg_70</textcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
    <textoffsetx>7</textoffsetx>
    <align>left</align>
    <aligny>center</aligny>
    <pulseonselect>no</pulseonselect>
  </default>
  <default type="scrollbar">
    <width>8</width>
    <texturesliderbackground colordiffuse="1Fededed" border="4">common/white.png</texturesliderbackground>
    <texturesliderbar colordiffuse="4Dededed" border="4">scrollbar/scrollv.png</texturesliderbar>
    <texturesliderbarfocus colordiffuse="$VAR[ColorHighlight]" border="4">scrollbar/scrollv.png</texturesliderbarfocus>
    <textureslidernib/>
    <textureslidernibfocus/>
    <showonepage>false</showonepage>
    <orientation>vertical</orientation>
  </default>
  <default type="grouplist">
    <itemgap>0</itemgap>
    <align>left</align>
  </default>
  <default type="slider">
    <texturesliderbar border="10" colordiffuse="dialog_fg_12">buttons/slider-back.png</texturesliderbar>
    <textureslidernib colordiffuse="dialog_fg_30">buttons/slider-nib.png</textureslidernib>
    <textureslidernibfocus colordiffuse="selected">buttons/slider-nib.png</textureslidernibfocus>
    <orientation>horizontal</orientation>
  </default>
  <default type="sliderex">
    <textoffsetx>7</textoffsetx>
    <aligny>center</aligny>
    <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">buttons/button-fo.png</texturefocus>
    <texturenofocus border="40">buttons/button-nofo.png</texturenofocus>
    <sliderwidth>150</sliderwidth>
    <sliderheight>28</sliderheight>
    <texturesliderbar>buttons/slider-back.png</texturesliderbar>
    <textureslidernib>buttons/slider-nib.png</textureslidernib>
    <textureslidernibfocus colordiffuse="$VAR[ColorHighlight]">buttons/slider-nib.png</textureslidernibfocus>
    <font>font_small_defaults</font>
    <textcolor>dialog_fg_70</textcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
  </default>
  <default type="mover">
    <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">buttons/button-fo.png</texturefocus>
    <texturenofocus border="40">buttons/button-nofo.png</texturenofocus>
  </default>
  <default type="resize">
    <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">buttons/button-fo.png</texturefocus>
    <texturenofocus border="40">buttons/button-nofo.png</texturenofocus>
  </default>
  <default type="edit">
    <align>center</align>
    <aligny>center</aligny>
    <font>font_small_defaults</font>
    <textoffsetx>30</textoffsetx>
    <textcolor>dialog_fg_70</textcolor>
    <focusedcolor>Selected</focusedcolor>
    <selectedcolor>$VAR[ColorHighlight]</selectedcolor>
    <disabledcolor>dialog_fg_12</disabledcolor>
    <texturefocus colordiffuse="dialog_bg_30">common/white.png</texturefocus>
    <texturenofocus/>
    <pulseonselect>false</pulseonselect>
  </default>
</includes>
